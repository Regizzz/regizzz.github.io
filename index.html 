        function myFunc() {
            let div = document.createElement('div');
            div.innerHTML = playlist[Math.floor(Math.random()*playlist.length)];
            document.getElementById("soundcloud").innerHTML = "";
            document.getElementById("soundcloud").append(div);
        }       
    </script>
